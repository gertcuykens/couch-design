{
 "_id":"_design/user",
 "language":"javascript",
 "validate_doc_update":"function(n,o,u){if(!n['_id'])throw({forbidden:'no _id'})}",
 "updates":{
  "put":"function(doc,req){
   var v=JSON.parse(req.body)
   for(i in v)doc[i]=v[i]
   var r={'headers':{'Content-Type':'application/json'},'body':JSON.stringify(doc)};
   return [doc,r]
  }"
 }
}
